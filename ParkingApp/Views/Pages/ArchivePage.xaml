        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Bottom">
            <Button Content="Назад" Name="Back" Click="Back_Click" Width="100" Margin="5"></Button>
        </StackPanel>
        <Grid Grid.Row="1">
            
        </Grid>
    </Grid>
</Page>
